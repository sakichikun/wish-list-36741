<div class="contents row">
  <div class="container">
    <h3>バケットリストの編集</h3>
    <%= form_with model: @want, local: true do |form| %>
      <%= form.text_field :name, placeholder: "Nickname" %>
      <%= form.text_field :title, placeholder: "A new bucket list" %>
      <%= form.submit "SEND" %>
      <%= link_to '戻る', wants_path %>
    <% end %>
  </div>
</div>